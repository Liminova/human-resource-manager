{
  "python.analysis.typeCheckingMode": "basic",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/__pycache__": true,
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  }
}